{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"]
  },
  "plugins": ["plugins/markdown", "better-docs/component", "better-docs/category"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/better-docs",
    "readme": "README.md"
  },
  "templates": {
    "better-docs": {
      "name": "Selector project"
    }
  }
}
